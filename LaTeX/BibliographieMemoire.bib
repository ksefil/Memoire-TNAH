@article{gabay2023,
  title = {{{OCR17}}: {{Ground Truth}} and {{Models}} for 17th c. {{French Prints}} (and hopefully more)},
  shorttitle = {{{OCR17}}},
  author = {Gabay, Simon and Clérice, Thibault and Reul, Christian},
  year = {2023},
  month = jun,
  journal = {Journal of Data Mining and Digital Humanities},
  volume = {2023},
  publisher = {INRIA},
  doi = {10.46298/jdmdh.6492},
  url = {https://hal.science/hal-02577236},
  urldate = {2024-09-05},
  abstract = {Machine learning begins with machine teaching: in the following paper, we present the data that we have prepared to kick-start the training of reliable OCR models for 17th century prints written in French. The construction of a representative corpus is a major challenge: we need to gather documents from different decades and of different genres to cover as many sizes, weights and styles as possible. Historical prints containing glyphs and typefaces that have now disappeared, transcription is a complex act, for which we present guidelines. Finally, we provide preliminary results based on these training data and experiments to improve them.},
  keywords = {17th c French,Construction de corpus,Corpus building,Data paper,Données,OCR,Training data,XVIIème siècle,OCRHTR}
}

@inproceedings{gabay2024,
  title = {Reconnaissance des écritures dans les imprimés},
  booktitle = {Humanistica 2024},
  author = {Gabay, Simon and Clérice, Thibault and Jacsont, Pauline and Leblanc, Elina and {Jeannot-Tirole}, Marie and Solfrini, Sonia and Dolto, Sophie and Goy, Floriane and Luján, Carmen Carrasco and Zaglio, Maddalena and Perregaux, Myriam and Janes, Juliette and Sagot, Benoît and Bawden, Rachel and Dent, Rasul and Nédey, Oriane and Chagué, Alix},
  year = {2024},
